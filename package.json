{
  "name": "vite-plugin-convex-types",
  "version": "1.0.2",
  "description": "A Vite plugin that automatically generates TypeScript types for Convex tables and function return types",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build",
    "publish": "npm run prepublishOnly && ./scripts/publish.sh"
  },
  "keywords": [
    "vite",
    "plugin",
    "convex",
    "typescript",
    "types",
    "generation",
    "database",
    "schema"
  ],
  "author": "Andrew Brough",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewBrough/vite-plugin-convex-types"
  },
  "bugs": {
    "url": "https://github.com/AndrewBrough/vite-plugin-convex-types/issues"
  },
  "homepage": "https://github.com/AndrewBrough/vite-plugin-convex-types#readme",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
